{
  "compilerOptions": {
    "baseUrl": "./app",
    "downlevelIteration": true,
    "outDir": "./dist/",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES6",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "noImplicitAny": false,
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react",
  },
  "include": [
    "app",
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
